{
    "name": "loterias-api/sdk",
    "description": "Official PHP SDK for Loterias API",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Loterias API",
            "email": "soporte@loterias-api.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "LoteriasApi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LoteriasApi\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "keywords": [
        "loterias",
        "loteria",
        "lottery",
        "spain",
        "euromillones",
        "primitiva",
        "bonoloto",
        "api",
        "sdk"
    ],
    "homepage": "https://loterias-api.com/docs/sdks",
    "support": {
        "issues": "https://github.com/loterias-api/sdk-php/issues",
        "docs": "https://loterias-api.com/docs/sdks"
    }
}
